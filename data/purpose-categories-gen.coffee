codePrefix = (level, name) ->
  code = switch level
    when 1
      switch name
        when "Social Infrastructure and Services" then "1"
        when "Economic Infrastructure and Services" then "2"
        when "Production Sectors" then "3"
        when "Multi-Sector/Cross-Cutting" then "4"
        when "Commodity Aid And General Program Assistance" then "5"
        when "Action Relating to Debt" then "6"
        when "Humanitarian Aid" then "7"
        when "Other" then "9"
    when 2
      switch name
        # ? Social Infrastructure and Services
        when "Education" then "11"
        when "Health" then "12"
        when "Population Policies/Programs and Reproductive Health" then "13"
        when "Water Supply and Sanitation" then "14"
        when "Government and Civil Society" then "15"
        when "Other Social Infrastructure and Services" then "16"

        # ￼Economic Infrastructure and Services
        when "Transport and Storage" then "21"
        when "Communications" then "22"
        when "Energy Generation and Supply" then "23"
        when "Banking and Financial Services" then "24"
        when "Business and Other Services" then "25"

        # Production Sectors
        when "Agriculture, Forestry, Fishing" then "31"
        when "Industry, Mining, Construction" then "32"

        # Multi-Sector/Cross-Cutting
        when "General Environmental Protection" then "41"
        when "Women" then "42"
        when "Other" then "43"

        # Commodity Aid And General Program Assistance
        when "General Budget Support" then "51"
        when "Development Aid/Food Security Assistance" then "52"
        when "Other Commodity Assistance" then "53"

        # Action Relating to Debt
        # Parent is the same
        #when "Action Relating to Debt" then "60"

        # Humanitarian Aid
        when "Emergency Response" then "72"
        when "Reconstruction Relief" then "73"
        when "Disaster Prevention and Preparedness" then "74"
        
        # Other?
        when "Administrative costs of Donors" then "91"
        when "Support to Non-Governmental Organizations and Government Organizations" then "92"
        when "Refugees in Donor Countries" then "93"
        when "Unallocated/Unspecified" then "99"
    when 3
      switch name
        when "Trade policy and regulations" then "331"
        when "Tourism" then "332"
        when "Education, level unspecified" then "111"
        when "Basic education" then "112"
        when "Secondary education" then "113"
        when "Post-secondary education" then "114"

        when "Health, general" then "121"
        when "Basic health" then "122"

        when "Government and civil society, general" then "151"
        when "Conflict prevention and resolution, peace and security" then "152"

        when "Agriculture" then "311"
        when "Forestry" then "312"
        when "Fishing" then "313"

        when "Industry" then "321"
        when "Mineral resources and mining" then "322"
        when "Construction" then "323"

  #console.log c = code ? ("? "+name)
  return c






recurse = (tree, level) ->

  values = (recurse(v, level+1) for v in tree.values when v.values?)

  t =
    name : tree.key
    prefix : ""+codePrefix(level, tree.key)

  if values.length > 0 then t.values = values

  return t
    

